{
    "builds": [{
        "src": "ecommerce/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.2" }
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "headers": {
                "cache-control": "max-age=31536000"
            },
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "ecommerce/wsgi.py"
        }
    ]

}